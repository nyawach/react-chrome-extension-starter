{
  "name": "react-chrome-extension-starter",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:nyawach/react-chrome-extension-starter.git",
  "author": "nyawach",
  "license": "MIT",
  "scripts": {
    "start": "webpack --watch",
    "build": "NODE_ENV=production webpack",
    "archive": "node ./scripts/archive.js",
    "test": "jest ./src/js --silent --verbose",
    "dashboard": "open https://chrome.google.com/webstore/developer/dashboard"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "resolver": "jest-webpack-resolver",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.77",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "@types/redux-logger": "^3.0.6",
    "@types/styled-components": "^4.1.5",
    "archiver": "^3.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-webpack-resolver": "^0.3.0",
    "prettier": "^1.15.3",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "react-test-renderer": "^16.7.0",
    "reducer-tester": "^1.0.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1",
    "webpack": "^4.27.0",
    "webpack-cli": "^3.1.2"
  }
}
